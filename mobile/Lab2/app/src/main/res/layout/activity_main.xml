<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/oneRental"
        android:layout_width="19dp"
        android:layout_height="25dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="444dp"
        android:text="Button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/twoRental"
        android:layout_width="17dp"
        android:layout_height="28dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="440dp"
        android:text="Button"
        app:layout_constraintStart_toEndOf="@+id/oneRental"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/threeRental"
        android:layout_width="18dp"
        android:layout_height="26dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="432dp"
        android:text="Button"
        app:layout_constraintStart_toStartOf="@+id/twoRental"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="434dp"
        android:layout_height="790dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/img" />

</androidx.constraintlayout.widget.ConstraintLayout>